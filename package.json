		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"esbuild-plugin-copy": "^2.1.1",
		"fs-extra": "^11.2.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
