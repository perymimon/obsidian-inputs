		"builtin-modules": "3.3.0",
		"cross-env": "^7.0.3",
		"esbuild": "0.17.3",
		"esbuild-plugin-copy": "^2.1.1",
		"fs-extra": "^11.2.0",
		"jest": "^29.7.0",
